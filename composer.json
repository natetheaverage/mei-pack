{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "laravel/cashier": "~6.0",
        "frozennode/administrator": " 5.*",
        "laracasts/utilities": "~2.0",
        "intervention/image": "dev-master",
        "spatie/laravel-analytics": "^1.3",
        "spatie/activitylog": "^2.4",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "barryvdh/laravel-snappy": "^0.3.0",
        "knplabs/knp-snappy": "^0.4.3",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "backup-manager/laravel": "^1.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-dropbox": "^1.0",
        "spatie/eloquent-sortable": "^2.1",
        "spatie/geocoder": "^2.1",
        "spatie/laravel-backup": "^3.2",
        "spatie/laravel-authorize": "^1.0",
        "spatie/laravel-permission": "^1.3",
        "orangehill/iseed": "dev-master",
        "predis/predis": "~1.1@dev",
        "iron-io/iron_mq": "~2.0",
        "kordy/ticketit": "^0.2.4"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
