lesscss-percentage(n)
  (n * 100)%
.btn-dashboard
  width 120px
  height 105px
  padding 20px 10px
  border-radius 7px
.dashboard-text
  display block
#dashboard-menu .badge
  position relative
  top -75px
#mainnav-menu li
  list-style-type none
.page-header
  font-size 2.6em
  padding 4px 0
.inline-block
  display inline-block
.accordion-tab-base
  margin-bottom 0
.accordion-tab-base > .tab-content
  color #eee
  background-color rgba(66, 91, 122, 0.43)
  border-top 1px solid #666
  border-right 1px solid #666
  border-bottom 1px solid #666
.accordion-tab-base > .nav-tabs > li.active > a, .accordion-tab-base > .nav-tabs > li.active > a:focus, .accordion-tab-base > .nav-tabs > li.active > a:hover
  color #eee
  cursor default
  background-color rgba(66, 91, 122, 0.43)
  border-top 1px solid #666
  border-left 1px solid #666
  border-bottom 1px solid #666
a.list-group-item, a.list-group-item:hover, a.list-group-item:focus
  color #333
a.list-group-item:hover
  background-color #f5f5f5
.list-group-item-heading
  font-size 2em
  font-weight 400
.panel .timeline
  background transparent
.timeline
  padding-bottom 0
.timeline:after
  left 0
  width 0
  height 0
  border-radius 0
.timeline-entry
  margin-bottom 15px
.timeline-label a.btn-link, .timeline-label a.btn-link:active
  color #337ab7
  text-decoration none
input:focus, textarea:focus
  outline thin #266fb0 solid
  outline-width 1px
.display-flex
  display flex
