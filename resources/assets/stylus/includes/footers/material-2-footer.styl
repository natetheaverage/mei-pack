/***********************
********* Footer ******
************************/
#Footer
	padding-top 0px
	padding-bottom 30px
	color #fff 
	background #2e2e2e
	a
		color #fff
		&:hover
			color $brand-orange
	ul
		list-style none
		padding 0
		margin 0 -7.5px
	> li
		display inline-block
		margin 0 7.5px
	label
		font-weight bold
		text-decoration underline
	li
		padding 0 10px

@media only screen and (min-width: 768px)
	#FSooter
		.social-icons
		float right
		.form-control
			box-shadow none
			-webkit-box-shadow none
